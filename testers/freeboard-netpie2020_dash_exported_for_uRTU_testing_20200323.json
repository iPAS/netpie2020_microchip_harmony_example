      "value": "datasources[\"ds_rtu\"][\"msg\"][\"dev_rtu\"][\"register\"][\"30000\"]",
      "animate": true
     }
    },
    {
     "type": "text_widget",
     "settings": {
      "title": "Log @msg",
      "size": "regular",
      "value": "datasources[\"ds_rtu\"][\"msg\"][\"dev_rtu\"][\"log\"]",
      "animate": true
     }
    }
